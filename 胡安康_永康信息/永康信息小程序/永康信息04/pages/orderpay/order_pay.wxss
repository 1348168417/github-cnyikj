.goods-list{ padding: 0px; background-color: #fff;}
.goods-list .good-item{ overflow: hidden; border-top: 1px solid #F4F4F4; padding: 6px 0; }
.goods-list .good-item:first-child{ border: none; padding-top: 0px; }
.goods-list .good-item:last-child{ padding-bottom: 0px; }
.good-item .left-img{ float: left; }
.good-item .left-img image{ width: 60px; height: 60px; }
.good-item .right-con{ float: left; padding-left: 10px; padding-top: 1px; font-size: 13px; }
.good-item .right-con view{ display: block; width: 225px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; }
.good-item .right-con view.category{ color: #969BA2; line-height: 1.6em; }
.good-item .right-con view>text{ position: absolute; right: 4px; color: #FD5F18; font-weight: bold; }
.good-item .right-con view:last-child{ margin-top: 13px; }

#control_sub, #total_count, #control_add, #control_stock {height: 1.5em;line-height: 1.5em;font-weight: bold;text-align: center;}
#control_sub, #control_add {width: 1.5em;border-top: 1px solid #e3e3e3;border-bottom: 1px solid #e3e3e3;background: -webkit-gradient(linear, center top, center bottom, from(#fff),to(#eeefee) );font-size: 1.4em;cursor: pointer;}
#control_sub {border-left: 1px solid #e3e3e3;}
#control_add {border-right: 1px solid #e3e3e3;}
