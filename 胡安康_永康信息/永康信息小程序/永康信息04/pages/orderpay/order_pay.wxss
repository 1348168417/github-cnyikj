#wjj_shop{ border: 1px solid #e0e0e0; }
 
.goods-info .pic-cover{width: 60px;height: 60px; background-color: #fff; -webkit-box-sizing: border-box;box-sizing: border-box;}
.goods-info .pic-cover>img{display: block;width: 100%;height: 100%;}
.goods-info .info{padding-left: 6em;}
.goods-info .info .goodsname{overflow: hidden;max-height: 4.3em;}
.control-cont{line-height: 2.5em;height: 2.5em;font-size: 1em;margin-top: 0.5em;}
