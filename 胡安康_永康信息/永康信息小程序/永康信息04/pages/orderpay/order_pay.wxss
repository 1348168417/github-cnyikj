.ticket_content, .point_redeem_content, .discount_content{
	display: none;
}

.ticket-price-setting{
	float: right;
	background: url(../../../youdian/image/mobile/arrow.png) no-repeat 0px -16px;
	background-size: 8px 30px;
	height: 15px;
	width: 10px;
	margin: 2px 0 0 10px;
}

.j-bg-mask{ display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background-color: #000; opacity: 0.7;}
.j-pop-box{ 
	display: none;
    position: fixed;
    top: 50%;
    width: 18rem;
    margin-top: -13.0rem;
    left: 50%;
    margin-left: -10rem;
    text-align: center;
    background-color: #F4F4F4;
    border-radius: 0.2rem;
    padding: 1rem;
}
.j-pop-box .box-title{ 
	font-size: 1.2rem; 
	margin: 1.0rem 0;
}

.j-pop-box .box-content{
	overflow-y: auto;
	width: 88%;
	margin: 10px 6%;
}

.j-pop-box .box-body{
	max-height: 300px;
	width: 94%;
}
.j-pop-box .box-body td{
	height: 41px;
	border-bottom: 1px solid #ababab;
	text-align: left;
}

.j-pop-box .box-body .coupon-price{
	line-height: 41px;
	font-size: 16px;
	color: #ff4400;
}

.j-pop-box .box-body .deadline{
    color: #999;
    font-size: 12px;
}

.microshop-box.remark-cont{ padding: 0px 12px 2px 12px;  }
