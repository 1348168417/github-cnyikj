<!--logs.wxml-->
<view class='tit'>
  <view class='tit'>『首席设计师造型设计作品展示』</view>
  
</view>
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" vertical="{{vertical}}" style="width:100%;height:490px;">
  <block wx:for="{{imgUrls}}" >
    <swiper-item class='tit_big'>
      <view class='tit_smell'>
        <view class='tit_a'>{{item.tit}}</view>
        <view class='tit_b'>{{item.arc}}</view>
      </view>
      <image src="{{item.img}}" class="slide-image"  />
    </swiper-item>
  </block>
</swiper>